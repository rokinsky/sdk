{
  "name": "@rokinsky/ui",
  "version": "1.0.0",
  "description": "TypeScript Client for Storyblok's API",
  "sideEffects": false,
  "keywords": [
    "sdk",
    "typescript",
    "storyblok",
    "cms",
    "client"
  ],
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "homepage": "https://github.com/rokinsky/sdk/tree/master/packages/ui#readme",
  "bugs": {
    "url": "https://github.com/rokinsky/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokinsky/sdk.git"
  },
  "author": "Aleksander Sorokin <ankezy@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c rollup.config.ts",
    "start": "NODE_ENV=development rollup -w -c rollup.config.ts",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint . --ext ts,js --max-warnings=0 --fix",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@rokinsky/eslint-config": "^1.0.0",
    "@rokinsky/prettier-config": "^1.0.0",
    "@rokinsky/tsconfig": "^1.1.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "eslint": "^7.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.34.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "prettier": "@rokinsky/prettier-config",
  "eslintConfig": {
    "extends": "@rokinsky/eslint-config"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "files": [
    "dist"
  ]
